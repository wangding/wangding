#!/usr/bin/env node

let msg = {
  name: '王顶',
  QQ: '408542507',
  Email: 'w.ding@qq.com',
  Github: 'https://github.com/wangding/',
  Bitbucket: 'https://Bitbucket.org/wngding',
  bilibili: 'https://space.bilibili.com/420780095',
};

msg = Object
  .entries(msg)
  .map(([k, v]) => (k+':').padEnd(12, ' ') + v)
  .join('\n');

console.log(msg);
